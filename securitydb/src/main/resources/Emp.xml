<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.sau.securitydb.Model.Emp">
        <table name="emp" schema="scott" catalog=""/>
        <attributes>
            <basic name="empno">
                <column name="EMPNO" precision="-1"/>
            </basic>
            <basic name="ename">
                <column name="ENAME" nullable="false" length="10"/>
            </basic>
            <basic name="job">
                <column name="JOB" nullable="false" length="9"/>
            </basic>
            <basic name="mgr">
                <column name="MGR" nullable="false" precision="-1"/>
            </basic>
            <basic name="hiredate">
                <column name="HIREDATE" nullable="false" precision="-1"/>
            </basic>
            <basic name="sal">
                <column name="SAL" nullable="false" precision="-1"/>
            </basic>
            <basic name="comm">
                <column name="COMM" nullable="false" precision="-1"/>
            </basic>
            <basic name="deptno">
                <column name="DEPTNO" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
    <entity class="com.sau.securitydb.Model.User">
        <table name="user" schema="websec" catalog=""/>
        <attributes>
            <basic name="id">
                <column name="id" precision="-1"/>
            </basic>
            <basic name="userName">
                <column name="user_name" length="16"/>
            </basic>
            <basic name="password">
                <column name="password" length="16"/>
            </basic>
            <basic name="roles">
                <column name="roles" length="32"/>
            </basic>
            <basic name="active">
                <column name="active" nullable="false" precision="1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
