<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.sau.securitydb.Model.User">
        <table name="user" schema="websec" catalog=""/>
        <attributes>
            <basic name="id">
                <column name="id" precision="-1"/>
            </basic>
            <basic name="userName">
                <column name="user_name" length="16"/>
            </basic>
            <basic name="password">
                <column name="password" length="16"/>
            </basic>
            <basic name="roles">
                <column name="roles" length="32"/>
            </basic>
            <basic name="active">
                <column name="active" nullable="false" precision="1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
