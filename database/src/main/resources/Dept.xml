<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.sau.project2.Model.Dept">
        <table name="dept" schema="scott" catalog=""/>
        <attributes>
            <basic name="deptno">
                <column name="DEPTNO" nullable="false" precision="-1"/>
            </basic>
            <basic name="dname">
                <column name="DNAME" nullable="false" length="14"/>
            </basic>
            <basic name="loc">
